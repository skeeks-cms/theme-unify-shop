{
	"name": "skeeks/theme-unify-shop",
	"description": "Theme for SkeekS CMS Project",
	"keywords": ["yii", "app", "skeeks", "framework", "theme"],
    "homepage": "https://cms.skeeks.com/",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
        "issues": "https://skeeks.com/",
        "wiki": "https://cms.skeeks.com/docs/"
    },
	"authors": [
		{
			"name": "Semenov Alexander",
			"email": "semenov@skeeks.com"
		}
	],
	"require": {
		"skeeks/cms-theme-unify-v2": "^2.0 || dev-master"
	},
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\themes\\unifyshop\\": "src/"
        }
    },
    "extra": {
		"config-plugin": {
			"web": [
				"src/config/common.php",
				"src/config/web.php"
			],
			"console": [
				"src/config/common.php",
				"src/config/console.php"
			]
		}
	}
}